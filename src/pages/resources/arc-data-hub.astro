---
import SubPageLayout from '../../layouts/SubpageLayout.astro';
import { type MarkdownInstance } from "astro";
const all_files : MarkdownInstance<Record<string,any>> [] = await Astro.glob('../../content/resources/arc-data-hub/*.md');
const hero : MarkdownInstance<Record<string,any>> = (await Astro.glob('../../content/resources/arc-data-hub/hero.md'))[0];
---
<SubPageLayout
  title="Resources - ARC Data Hub"
  hero={hero}
  cards={all_files}>
</SubPageLayout>